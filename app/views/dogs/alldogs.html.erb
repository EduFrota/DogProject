<p id="notice"><%= notice %></p>

<h1>Dogs</h1>

<table>
  <thead>
    <tr>
      <th>Imagem</th>
      <th>Name</th>
      <th>Breed</th>
      <th>User</th>
      <th colspan="4"></th>
    </tr>
  </thead>

  <tbody>
    <% @dogs.each do |dog| %>
      <tr>
        <td><%= image_tag(dog.image_url(:thumb).to_s) %></td>
        <td><%= dog.name %></td>
        <td><%= dog.breed %></td>
        <td><%= dog.user.name %></td>
        <td><%= link_to 'Show', dog %></td>

      </tr>
    <% end %>
  </tbody>
</table>

<br>
