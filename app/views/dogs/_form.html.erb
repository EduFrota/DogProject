<%= simple_form_for @dog do |f| %>
  <%= f.input :image %>
  <%= f.input :name %>
  <%= f.input :breed %>
  <%= f.input :age %>
  <%= f.input :gender %>
  <%= f.input :vacine %>
  <%= f.input :pedigree %>
  <%= f.input :mother_name %>
  <%= f.input :father_name %>
  <%= f.input :register %>
  <%= f.input :temperament %>
  <%= f.input :length %>
  <%= f.input :heigth %>
  <%= f.input :pelage %>
  <%= f.input :pelage_color %>
  <%= f.input :weight %>


  <%= f.button :submit %>
<% end %>
