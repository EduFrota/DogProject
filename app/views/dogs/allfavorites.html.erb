<header>
  <%= render "shared/navbarTabs" %>
</header>




<table>
  <thead>
    <tr>
      <th></th>
      <th>Name</th>
      <th>Breed</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% current_user.favorites.each do |favorite| %>
      <tr>
        <td><%= image_tag(favorite.dog.image_url(:thumb).to_s) %></td>
        <td><%= favorite.dog.name %></td>
        <td><%= favorite.dog.breed %></td>
        <td><%= link_to 'Show', favorite.dog %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<br>
